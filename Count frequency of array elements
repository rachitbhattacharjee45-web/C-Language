#include <stdio.h>
int main() {
    int arr[] = {1, 2, 2, 3, 1, 4, 2, 3};
    int size = sizeof(arr) / sizeof(arr[0]);
    int i, j;

    printf("Element | Frequency\n");
    printf("-------------------\n");
    for (i = 0; i < size; i++) {
        int count = 0;
        for (j = 0; j < size; j++) {
            if (arr[i] == arr[j]) {
                count++;
            }
        }

        printf("   %d    |    %d\n", arr[i], count);
    }

    return 0;
}
