#include <stdio.h>
int main() {
    int arr[] = {1, 2, 2, 3, 1, 4, 2, 3};
    int size = sizeof(arr) / sizeof(arr[0]);
    int visited[size];
    int i, j;
    for (i = 0; i < size; i++) {
        visited[i] = 0;
    }
    printf("Element | Frequency\n");
    printf("-------------------\n");

    for (i = 0; i < size; i++) {
        if (visited[i] == 1)
            continue;

        int count = 1;
        for (j = i + 1; j < size; j++) {
            if (arr[i] == arr[j]) {
                count++;
                visited[j] = 1;
            }
        }

        printf("   %d    |    %d\n", arr[i], count);
    }

    return 0;
}
