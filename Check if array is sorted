#include <stdio.h>

int isSortedAscending(int arr[], int size) {
    for (int i = 0; i < size - 1; i++)
        if (arr[i] > arr[i + 1])
            return 0; // false
    return 1; // true
}

int main() {
    int arr[] = {1, 2, 3, 4, 5};
    int size = sizeof(arr) / sizeof(arr[0]);

    printf("The array is %ssorted in ascending order.\n",
           isSortedAscending(arr, size) ? "" : "NOT ");

    return 0;
}
